{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "strictNullChecks": true,
    "baseUrl": "src",
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["*"],
      "@lib/*": ["lib/*"],
      "@utils/*": ["utils/*"],
      "@components/*": ["components/*"],
      "@layouts/*": ["layouts/*"],
      "@assets/*": ["assets/*"],
      "@logos/*": ["assets/logos/*"],
      "@pages/*": ["pages/*"],
      "@i18n/*": ["i18n/*"],
      "@styles/*": ["styles/*"]
    }
  }
}
